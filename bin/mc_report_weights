#!/usr/bin/env python

"""\
%prog < theta.pkl

Show relative critic weights.
"""

import sys
import argparse
import pickle

from metacritic.metacritic import report

if __name__ == '__main__':
    parser = argparse.ArgumentParser(description=__doc__)
    args = parser.parse_args()

    theta = pickle.loads(sys.stdin.read())
    report(theta)
